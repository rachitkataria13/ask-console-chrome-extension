{
  "manifest_version": 3,
  "name": "Alexa Skills Kit Simulator Enhancer",
  "description": "Speed up Alexa Skill testing in the Alexa Skill Console",
  "version": "1.5",
  "icons": {
    "48": "/img/icon_48.png",
    "128": "/img/icon_128.png"
  },
  "content_scripts": [{
    "matches": ["https://developer.amazon.com/alexa/console/ask/*"],
    "js": ["jquery-3.7.1.min.js", "jquery-ui.min.js", "content.js", "pageIndex.js", "pageTest.js"],
    "css": ["style.css"]
  }],
  "permissions": ["storage"]
}
